{testCase} from ./helper.civet

describe "throw", ->
  testCase """
    throw value
    ---
    throw x
    ---
    throw x
  """

  testCase """
    throw nested object
    ---
    throw
      a: b
    ---
    throw {
      a: b,
    }
  """

  testCase """
    throw with postfix conditional
    ---
    throw x if y
    ---
    if (y)throw x
  """
