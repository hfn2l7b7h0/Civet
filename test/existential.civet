{testCase} from ./helper.civet

describe "existential", ->
  // TODO: only add parens when necessary given context
  testCase """
    default
    ---
    a?
    ---
    (a != null)
  """

  testCase """
    after access
    ---
    a.b?
    ---
    (a.b != null)
  """

  testCase """
    with binary operator
    ---
    count + item?
    ---
    count + (item != null)
  """
