import {testCase} from "./helper.civet"

describe "coffee compat", ->
  testCase """
    ==
    ---
    "use coffee-compat"
    a == b
    ---
    "use coffee-compat"
    a === b;
  """

  testCase """
    !=
    ---
    "use coffee-compat"
    a != b
    ---
    "use coffee-compat"
    a !== b;
  """
